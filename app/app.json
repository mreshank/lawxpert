{
  "expo": {
    "name": "LawXpert",
    "slug": "lawxpert",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "LawXpert",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash/splash-screen.png",
      "resizeMode": "cover",
      "backgroundColor": "#2c2a56"
    },
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.lawxpert.app",
      "permissions": [],
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/splash/splash-screen.png",
          "resizeMode": "cover",
          "backgroundColor": "#2c2a56"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "b2f1576f-e672-49dc-9efb-71cd3d5047c7"
      }
    },
    "owner": "mreshank"
  }
}
