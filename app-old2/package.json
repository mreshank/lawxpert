{
  "name": "lawxpert-mobile",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "start:macos": "ulimit -n 10240 && expo start",
    "start:optimized": "expo start --no-dev --minify",
    "start:lightweight": "REACT_NATIVE_MAX_WORKERS=2 NODE_OPTIONS=--max-old-space-size=2048 expo start --dev-client --minify",
    "start:simple": "EXPO_ENTRYPOINT=./SimpleApp.js expo start"
  },
  "dependencies": {
    "eas-cli": "^16.5.0",
    "expo": "~53.0.0",
    "expo-router": "~3.4.7",
    "expo-splash-screen": "~0.26.4",
    "expo-status-bar": "~1.11.1",
    "expo-updates": "~0.24.9",
    "react": "18.2.0",
    "react-native": "0.73.4",
    "react-native-safe-area-context": "4.8.2",
    "react-native-screens": "~3.29.0",
    "react-native-webview": "13.6.4"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0"
  },
  "private": true
}
